{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always"],
        "no-console": "off",
        "no-unused-vars": "error",
        "no-use-before-define": "error",
        "object-shorthand": ["error", "always"],
        "camelcase": "off"
    }
}
